{
    "name": "Onatrix RTE",
    "alias": "Onatrix.Rte",
    "extensions": [
        {
            "type": "tiptapToolbarExtension",
            "kind": "styleMenu",
            "alias": "Onatrix.Tiptap.StyleMenu",
            "name": "Onatrix Tiptap Style Menu",
            "meta": {
                "alias": "OnatrixStyleMenu",
                "icon": "icon-palette",
                "label": "Onatrix Styles"
            },
            "items": [
                {
                    "label": "Paragraph Sizes",
                    "items": [
                        {
                            "label": "XL (20px)",
                            "data": {
                                "tag": "p",
                                "class": "text-xl"
                            }
                        },
                        {
                            "label": "L (18px)",
                            "data": {
                                "tag": "p",
                                "class": "text-lg"
                            }
                        },
                        {
                            "label": "M (16px)",
                            "data": {
                                "tag": "p",
                                "class": "text-base"
                            }
                        },
                        {
                            "label": "S (14px)",
                            "data": {
                                "tag": "p",
                                "class": "text-sm"
                            }
                        }
                    ]
                },
                {
                    "label": "Font Weight",
                    "items": [
                        {
                            "label": "Light",
                            "data": {
                                "tag": "p",
                                "class": "font-light"
                            }
                        },
                        {
                            "label": "Normal",
                            "data": {
                                "tag": "p",
                                "class": "font-normal"
                            }
                        },
                        {
                            "label": "Bold",
                            "data": {
                                "tag": "p",
                                "class": "font-bold"
                            }
                        },
                        {
                            "label": "Italic",
                            "data": {
                                "tag": "p",
                                "class": "italic"
                            }
                        }
                    ]
                },
                {
                    "label": "Headings",
                    "items": [
                        {
                            "label": "H1",
                            "data": {
                                "tag": "h1",
                                "class": "text-4xl font-bold text-primary"
                            }
                        },
                        {
                            "label": "H2",
                            "data": {
                                "tag": "h2",
                                "class": "text-3xl font-semibold text-primary"
                            }
                        },
                        {
                            "label": "H3",
                            "data": {
                                "tag": "h3",
                                "class": "text-2xl font-semibold text-primary"

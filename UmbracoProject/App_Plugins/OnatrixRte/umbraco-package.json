                        {
                            "label": "H5",
                            "data": {
                                "tag": "h5",
                                "class": "text-lg font-medium text-primary"
                            }
                        }
                    ]
                },
                {
                    "label": "Lists",
                    "data": {
                        "tag": "ul",
                        "class": "list-disc list-inside ml-4"
                    }
                },
                {
                    "label": "Images",
                    "items": [
                        {
                            "label": "Full Width",
                            "data": {
                                "tag": "img",
                                "class": "w-full h-auto rounded-lg"
                            }
                        }
                    ]
                }
            ]
        },
